/**
 * @param {number[]} nums
 * @return {number}
 */
var minimumOperations = function (nums) {
    const hash = new Map();
    let counter = 0;


    nums.forEach(num => {
        if (num !== 0) {
            if (!hash.has(num)) {
                hash.set(num, 1);
                counter ++
            }
        }
    })

    return counter;
};
