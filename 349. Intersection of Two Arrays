/**
 * @param {number[]} nums1
 * @param {number[]} nums2
 * @return {number[]}
 */
var intersection = function (nums1, nums2) {
    let map1 = new Set(nums1)
    let map2 = new Set(nums2)
    let result = new Set()
    map1.forEach((num) => {
        map2.has(num) === true ? result.add(num) : null
    })
    return [...result]
};
