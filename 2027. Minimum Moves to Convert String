/**
 * @param {string} s
 * @return {number}
 */
var minimumMoves = function(s) {
    const ar = s.split('')
    let i = 0;
    let moves = 0;
    while (i < s.length) {
        if (ar[i] === "X") {
            i += 3;
            moves += 1
        } else {
            i += 1
        }
    }
    return moves;
};
