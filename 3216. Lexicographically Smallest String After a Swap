function getSmallestString(s: string): string {
    const strAr = s.split('');

    for (let i = 0; i < strAr.length - 1; i++) {
        const el = +strAr[i];
        const elNext = +strAr[i + 1];
        let shouldSwap = false;
        if (el % 2 === 0 && elNext % 2 === 0 && elNext < el) {
            shouldSwap = true;
        }
        if (el % 2 !== 0 && elNext % 2 !== 0 && elNext < el) {
            shouldSwap = true;
        }

        if (shouldSwap) {
            s = s.substr(0,i) + s[i+1] + s[i] + s.substr(i+2);;
            return s;
        }
    }

    return s;
};
